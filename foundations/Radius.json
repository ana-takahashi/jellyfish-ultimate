{
  "jf": {
    "radius": {
      "sm": {
        "value": "round({jf.radius.md} / ({jf.radius.scale-ratio} + 0.33))",
        "type": "borderRadius"
      },
      "md": {
        "value": "{jf.radius.base}",
        "type": "borderRadius"
      },
      "lg": {
        "value": "round({jf.radius.md} * {jf.radius.scale-ratio})",
        "type": "borderRadius"
      },
      "xl": {
        "value": "round({jf.radius.md} * (1.5*{jf.radius.scale-ratio}))",
        "type": "borderRadius"
      },
      "xl2": {
        "value": "round({jf.radius.md} * (2.5*{jf.radius.scale-ratio}))",
        "type": "borderRadius"
      },
      "xl3": {
        "value": "round({jf.radius.md} * (4*{jf.radius.scale-ratio}))",
        "type": "borderRadius"
      },
      "g": {
        "value": "round({jf.radius.md} * (5*{jf.radius.scale-ratio}))",
        "type": "borderRadius"
      },
      "pill": {
        "value": "999px",
        "type": "borderRadius"
      },
      "sharp": {
        "value": "0",
        "type": "borderRadius"
      },
      "circle": {
        "value": "50%",
        "type": "borderRadius"
      }
    },
    "surface": {
      "radius": {
        "sharp": {
          "value": "{jf.radius.sharp}",
          "type": "borderRadius"
        },
        "rounded": {
          "md": {
            "value": "{jf.radius.xl}",
            "type": "borderRadius"
          },
          "lg": {
            "value": "{jf.radius.xl2}",
            "type": "borderRadius"
          }
        }
      }
    }
  }
}